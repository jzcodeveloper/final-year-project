{
  "name": "ml-crowdsourcing-platform",
  "description": "Crowdsourcing platform to feed ML models built with TensorflowJS.",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "config": "^3.2.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^1.3.2",
    "express-sslify": "^1.2.0",
    "express-zip": "^3.0.0",
    "helmet": "^3.21.1",
    "hpp": "^0.2.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.7.11",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "socket.io": "^2.3.0",
    "xss-clean": "^0.1.1"
  },
  "author": "jzcodeveloper",
  "license": "MIT",
  "nodemonConfig": {
    "ignore": [
      "client/**"
    ]
  }
}
